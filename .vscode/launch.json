   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
       {
           "name": "Sample.ASPNetCore",
           "type": "coreclr",
           "request": "launch",
           "preLaunchTask": "Build Hangfire.Mongo.Sample.ASPNetCore",
           "program": "${workspaceRoot}/src/Hangfire.Mongo.Sample.ASPNetCore/bin/Debug/netcoreapp2.0/Hangfire.Mongo.Sample.ASPNetCore.dll",
           "args": [],
           "cwd": "${workspaceRoot}/src/Hangfire.Mongo.Sample.ASPNetCore",
           "stopAtEntry": false,
           "launchBrowser": {
               "enabled": true,
               "args": "${auto-detect-url}",
               "windows": {
                   "command": "cmd.exe",
                   "args": "/C start ${auto-detect-url}"
               },
               "osx": {
                   "command": "open"
               },
               "linux": {
                   "command": "xdg-open"
               }
           },
           "env": {
               "ASPNETCORE_ENVIRONMENT": "Development"
           },
           "sourceFileMap": {
               "/Views": "${workspaceRoot}/src/Hangfire.Mongo.Sample.ASPNetCore/Views"
           }
       },
        {
            "name": "Sample.NETCore",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Hangfire.Mongo.Sample.NETCore",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceRoot}/src/Hangfire.Mongo.Sample.NETCore/bin/Debug/netcoreapp2.0/Hangfire.Mongo.Sample.NETCore.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/Hangfire.Mongo.Sample.NETCore",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "externalTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}